{
  "short_name": "Kassenzettel",
  "name": "Kassenzettel",
  "version": "1.0.2",
  "manifest_version": 3,
  "description": "LÃ¤dt Kassenzettel von Migros und fasst die Ausgaben zusammen.",
  "icons": {
    "48": "logo48.png",
    "128": "logo128.png"
  },
